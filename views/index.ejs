

<a href="/auth/signup" class="btn btn-info">Signup</a>
<a href="/auth/login" class="btn btn-info">Login</a>
<a href="/auth/logout" class="btn btn-info">Logout</a>
<a href="/profile" class="btn btn-info">Profile</a>


<h2>Now Playing</h2>

<div class="well">

<audio controls>
  <source src="http://live-aacplus-64.kexp.org/kexp64.aac" type="audio/mp4" />
</audio>

<h3> <%= currentSong.Artist.Name %> - <%= currentSong.Track.Name %></h3>
</div>

<div class="row">
<div class="col-md-8">
  <div class="well">
  <% recentPlays.Plays.forEach(function(song){ %>
  <% if(song.Artist) { %>
  <h4> <a href="#" data-song="<%= song.CreatedDate %>" class="btn btn-danger"> <i class="fa fa-heart"></i> </a>  <%=  song.Artist.Name %> - <%=  song.Track.Name %> </h4>

  <hr>
   <% } %>
  <% }); %>
  </div>

</div>
</div>


